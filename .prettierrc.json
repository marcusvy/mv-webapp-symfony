{
	"arrowParens": "always",
	"bracketSpacing": true,
	"bracketSameLine": true,
	"printWidth": 120,
	"proseWrap": "always",
	"semi": false,
	"singleQuote": true,
	"singleAttributePerLine": true,
	"tabWidth": 2,
	"trailingComma": "all",
	"twigPrintWidth": 120,
	"twigMultiTags": ["nav,endnav", "switch,case,default,endswitch", "ifchildren,endifchildren", "cache,endcache"],
	"useTabs": true,
	"plugins": ["./node_modules/prettier-plugin-twig-melody"]
}
